        "configuration": {
            "vpcId": "${vpc.resourceId}"
        },
        "relationships": []
    },
    "subnet2": {
        "id": "${subnet2.arn}",
        "arn": "arn:aws:ec2:${$constants.region}:${$constants.accountId}:subnet/${$constants.subnet.id2}",
